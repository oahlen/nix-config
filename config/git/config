    autocrlf = "input"
    editor = "nvim"
    eol = "lf"

[filter "lfs"]
    clean = "git-lfs clean -- %f"
    process = "git-lfs filter-process"
    required = true
    smudge = "git-lfs smudge -- %f"

[init]
    defaultBranch = "main"

[pull]
    rebase = "true"

[push]
    autoSetupRemote = "true"

